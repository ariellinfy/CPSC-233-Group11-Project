<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="260.0" prefWidth="445.0" stylesheets="@../resources/gameOverviewStyling.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.GameOverController">
	<bottom>
		<HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
			<children>
				<Button mnemonicParsing="false" onAction="#onExit" prefWidth="165.0">
					<font>
						<Font size="18.0" />
					</font>
					<HBox.margin>
						<Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
					</HBox.margin>
					<padding>
						<Insets bottom="9.0" left="10.0" right="10.0" top="9.0" />
					</padding>
               <graphic>
                  <ImageView fitHeight="22.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../resources/Black-X-Letter.png" />
                     </image>
                  </ImageView>
               </graphic>
				</Button>
				<Button fx:id="buttonRestart" mnemonicParsing="false" onAction="#onRestart" prefWidth="165.0">
					<font>
						<Font size="18.0" />
					</font>
					<HBox.margin>
						<Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
					</HBox.margin>
					<padding>
						<Insets bottom="9.0" left="10.0" right="10.0" top="9.0" />
					</padding>
               <graphic>
                  <ImageView fitHeight="22.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../resources/Play-Graphic.png" />
                     </image>
                  </ImageView>
               </graphic>
				</Button>
			</children>
		</HBox>
	</bottom>
	<center>
		<VBox alignment="CENTER" prefHeight="144.0" prefWidth="445.0" BorderPane.alignment="CENTER">
			<children>
            <ImageView fx:id="winningImage" fitHeight="80.0" fitWidth="277.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin></ImageView>
				<Label fx:id="labelScore" alignment="CENTER" contentDisplay="CENTER" prefHeight="60.0" prefWidth="375.0" textAlignment="CENTER" wrapText="true">
					<font>
						<Font size="18.0" />
					</font>
				</Label>
			</children>
		</VBox>
	</center>
</BorderPane>
